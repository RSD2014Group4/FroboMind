<!-- frobit launch file 

FroboMind controller frobits using 22000/3300 ohm voltage dividers:
	supply_voltage_scale_factor = 0.03747

RoboCard based frobits using 1800/700 ohm voltage dividers: 
	supply_voltage_scale_factor = 0.01746

-->

<launch>

 	<arg name="sim" default="false" />

	<node pkg="usb_cam" name="usb_cam" type="usb_cam_node" />  
	
	<node pkg="line_nodes" name="line_det" type="line_det" /> 

	<node pkg="line_nodes" name="barcode_det" type="barcode_det" />

	<node pkg="pid_controller" name="lineFollowFSM" type="lineFollowFSM.py" /> 

	<node pkg="line_til_cross_action" name="Gocell" type="line_server" output="screen" /> 

	<node pkg="spin_90_degrees" name="spin_server" type="spin_server" output="screen" /> 

	<include file="$(find TurboUlla)/launch/include/imu.launch"/>

  	<include file="$(find TurboUlla)/launch/include/platform.launch">
	    <arg name="sim" value="$(arg sim)"/>
	  </include>


</launch>

